<div class="new-page property-summary">
    <div class="page-header">
      <%- chapter %>
    </div>
  
    <div class="details-body">
      <table class="top-table">
        <tbody>
          <tr>
            <td class="info-title">Property <span class="section-title">Boundary</span></td>
          </tr>
          <tr>
            <td class="image-area">
              <% if (evaluation.map_image_url) { %>
                <img 
                  class="image image-added" 
                  src="<%= evaluation.map_image_url %>" 
                  data-image-name="PROPERTY_IMAGE_DETAILS"
                >
              <% } else { %>
                <img 
                  class="image" 
                  src="/images/default-placeholder.png"
                >
              <% } %>
            </td>
          </tr>
        </tbody>
      </table>
    </div>
  
    <%- watermark %>
  </div>
  